<!DOCTYPE html>
<html>
    <head>

        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

        <title>{% block title %}FreeQuest{% endblock %}</title>

        <link rel="icon" type="image/x-icon" href="{{ asset('FqBundle::images/favicon.ico') }}" />

        {% block stylesheets %}
            <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css" rel="stylesheet" >
            <link href="//cdnjs.cloudflare.com/ajax/libs/bootstrap-social/4.2.1/bootstrap-social.min.css" rel="stylesheet">
            <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
            <link href="{{ asset('FqBundle::public/css/styles.css') }}" rel="stylesheet" type="text/css" />
        {% endblock %}

    </head>
    <body>

        {{ include('FqBundle::partials/header.html.twig') }}

        {% block body %}{% endblock %}

        {{ include('FqBundle::partials/footer.html.twig') }}

        {% block javascripts %}
            <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
            <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>
        {% endblock %}

    </body>
</html>
